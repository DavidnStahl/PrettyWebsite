@model PrettyWebsite.Models.ViewModels.Blocks.MovieReviewBlockViewModel

@{
    Layout = null;
}
<div class="container text-dark bg-warning">
    <div class="row">
        <div>
            <h1 class="text-center mt-3 mb-3">@Html.PropertyFor(m => m.CurrentBlock.Title)</h1>

            <div id="partialViewReviewResult"></div>
        </div>

    </div>
</div>

<script>
    function play() {
        $.get("/MovieReviewResult/Index",
            function (data, status) {
                $("#partialViewReviewResult").empty().append(data);
                //alert("Data: " + data + "\nStatus: " + status);
            });
    };

    play();




</script>
