@using PrettyWebsite.Models.Forms

@model SearchFormModel

@{
    Layout = null;
}



@if (Model.SearchResult.Search.Any())
{
    foreach (var movie in Model.SearchResult.Search)
    {
        <option class="SearchTextOption" value="@movie.Title" data-href="@Url.Action("","Movie",new { id = movie.imdbID })"/>
    }
}


